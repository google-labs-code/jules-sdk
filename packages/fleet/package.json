  "author": "Google LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "@google/jules-sdk": "^0.0.6",
    "citty": "^0.1.6",
    "octokit": "^4.1.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^22.15.0",
    "typescript": "^5.8.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
